<script type="7775d645538ed7aab106396d-text/javascript">
$(function(){

	function gocode(){
		if ($("#qcode").val() != ""){
			window.location.href = "/chargemaster/code/" + $("#qcode").val();
		}
	}
	
	$("#gocode").click(gocode);
	
	$("#qcode").on('keypress',function(e) {
	    if(e.which == 13) {
	        gocode();
	    }
	});
	
	
	

	function godesc(){
		if ($("#qdesc").val() != ""){
			window.location.href = "/chargemaster/desc?qdesc=" + $("#qdesc").val();
		}
	}
	
	$("#godesc").click(godesc);
	
	$("#qdesc").on('keypress',function(e) {
	    if(e.which == 13) {
	        godesc();
	    }
	});
	
	$(".tablesorter").tablesorter();
});


</script>


